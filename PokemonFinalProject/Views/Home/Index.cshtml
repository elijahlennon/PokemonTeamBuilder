@{
    ViewData["Title"] = "Home Page";
}

<div class="team-container">
    @{
        for (int i = 0; i < 6; i++)
        {
            <a id=@i class="team-member" onclick="createModal(event)"></a>
        }
    }
</div>

<div>
    <div>
        <input oninput="searchPokemon(event)" id="searchInput" />
    </div>
    <div id="searchResults" />
</div>

